{
  "version": "2.0",
  "envId": "{{env.envId}}",
  "framework": {
    "name": "daiwa-cn",
    "plugins": {
      "web": {
        "use": "@cloudbase/framework-plugin-website",
        "inputs": {
          "installCommand": "cd apps/site && (npm ci || npm i)",
          "buildCommand": "cd apps/site && npm run build",
          "outputPath": "apps/site/dist",
          "cloudPath": "/daiwa-cn-site/",
          "ignore": [ ".git", ".github", "node_modules", "cloudbaserc.js" ]
        }
      }
    }
  }
}
